{
  "name": "thally",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "presetup": "npm i shelljs",
    "setup": "node ./setup.js",
    "clean": "rm -rf dist",
    "build:webpack": "NODE_ENV=production webpack --config ./config/webpack.config.js --color  --progress",
    "build": "npm run clean && npm run build:webpack",
    "postsetup": "rm -rf ./setup.js && npm un shelljs",
    "start": "webpack-dev-server --config ./config/webpack.config.js",
    "test": "ava \"test/**/*.test.js\" --verbose",
    "test:dev": "ava \"test/**/*.test.js\" --verbose --watch",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "./test/setup-test-env.js"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/osmanov/thally.git"
  },
  "author": "Renat Osmanov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/osmanov/thally/issues"
  },
  "homepage": "https://github.com/osmanov/thally#readme",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^10.0.0",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "^1.3.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "ip": "^1.1.5",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0"
  }
}
