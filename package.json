  },
  "homepage": "https://github.com/osmanov/thally#readme",
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.18.0",
    "enzyme": "^2.5.1",
    "jsdom": "^9.8.3",
    "react-addons-test-utils": "^15.3.2",
